{
  "path": [
    "types/",
    "lib/",
    "components/",
    "app/",
    "hooks/"
  ],
  "languages": ["typescript", "tsx"],
  "ignore": [
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/.next/**",
    "**/coverage/**",
    "**/.git/**",
    "**/docs/**"
  ],
  "reporters": ["console", "html"],
  "minLines": 3,
  "minTokens": 15,
  "threshold": 0.1,
  "output": "./jscpd-report"
}
