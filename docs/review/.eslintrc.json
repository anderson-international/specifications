{
  "env": {
    "node": true,
    "es2022": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-html-link-for-pages": "off",
    "no-unused-vars": "warn",
    "no-console": "off"
  },
  "ignorePatterns": [
    "tests/**",
    "output/**"
  ]
}
